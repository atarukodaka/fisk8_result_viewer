keys = [:name, :competition_name, :date, :season, :ranking, :skater_name, :nation,
        :number, :name, :credit, :info, :base_value, :goe, :judges, :value]

csv << keys


collection.each do |item|
  csv << [item.score.name, item.score.competition.name, item.score.date, item.score.competition.season,
          item.score.ranking, item.score.skater.name, item.score.skater.nation] +
    ([:number, :name, :credit, :info, :base_value, :goe, :judges, :value].map {|k| item[k]})
end
